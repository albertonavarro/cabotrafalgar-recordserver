<beans xmlns="http://www.springframework.org/schema/beans" 
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
       xmlns:jaxws="http://cxf.apache.org/jaxws" 
       xsi:schemaLocation=" http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd \
http://cxf.apache.org/jaxws http://cxf.apache.org/schema/jaxws.xsd">
    
    <bean id="client" class="com.navid.login.SystemCommands" factory-bean="clientFactory" factory-method="create"/>
    
    <bean id="clientFactory" class="org.apache.cxf.jaxws.JaxWsProxyFactoryBean">
        <property name="serviceClass" value="com.navid.login.SystemCommands"/>
        <property name="address" value="http://lazylogin.ws:8080/system"/>
        <property name="handlers">
            <list>
                <bean class="com.navid.trafalgar.recordserver.endpoints.RequestIdHandler"/>
            </list>
        </property>
    </bean>

</beans>